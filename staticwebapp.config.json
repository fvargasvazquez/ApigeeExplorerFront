{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/js/*",
            "/*.{js,css,json,ico,svg,woff,woff2,ttf,eot}"
        ]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".woff": "application/font-woff",
        ".woff2": "application/font-woff2"
    },
    "globalHeaders": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "SAMEORIGIN",
        "X-XSS-Protection": "1; mode=block"
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ]
}